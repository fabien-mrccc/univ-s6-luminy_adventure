# Normalize EOL for all files that Git considers text files.
* text=auto eol=lf
*.glb filter=lfs diff=lfs merge=lfs -text
assets/sounds/*.mp3 filter=lfs diff=lfs merge=lfs -text
scenes/world/*.tscn filter=lfs diff=lfs merge=lfs -text
scenes/world/map.tscn filter=lfs diff=lfs merge=lfs -text
*.tscn filter=lfs diff=lfs merge=lfs -text
'*.tscn' filter=lfs diff=lfs merge=lfs -text
